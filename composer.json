{
  "name": "smartbooster/content-bundle",
  "type": "symfony-bundle",
  "description": "SMART BOOSTER content bundle",
  "keywords": [],
  "homepage": "https://github.com/smartbooster/content-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Bastien",
      "email": "nicolas.bastien@smartbooster.io",
      "homepage": "https://www.smartbooster.io"
    }
  ],
  "require": {
    "doctrine/doctrine-bundle": "^1.6",
    "smartbooster/sonata-bundle": "^1.0@dev",
    "stof/doctrine-extensions-bundle": "^1.3",
    "vich/uploader-bundle": "^1.13",
    "liip/imagine-bundle": "^2.3"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "sebastian/phpcpd": "^2.0",
    "phpmetrics/phpmetrics": "^2.5",
    "phpstan/phpstan": "^0.12.3",
    "phpstan/phpstan-doctrine": "^0.12.4",
    "phpstan/phpstan-symfony": "^0.12.1",
    "dama/doctrine-test-bundle": "^6.3",
    "phpunit/phpunit": "^8.5",
    "doctrine/doctrine-fixtures-bundle": "^3.3",
    "theofidry/alice-data-fixtures": "^1.1",
    "liip/test-fixtures-bundle": "^1.0.0",
    "symfony/phpunit-bridge": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Smart\\ContentBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Smart\\ContentBundle\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
